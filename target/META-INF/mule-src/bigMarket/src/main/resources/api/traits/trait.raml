#%RAML 1.0 Trait
headers:
  client_id:
    type: string
    minLength: 5
  client_secret:
    type: string
    minLength: 5

responses:
  400:
    description: Token was not provided
  401:
    description: Bad or expired tocken. To fix, you should reauthenticate the user
  403:
    description: The client id validation failed
  503:
    description: Error communicating with server
